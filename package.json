{
  "name": "polinavite",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:static": "npm run build && node scripts/static-generation.js",
    "build:gh-pages": "tsc && vite build --base=/polinavite/ && node scripts/static-generation.js",
    "preview": "vite preview",
    "optimize-images": "node scripts/optimize-images.mjs",
    "generate-sitemap": "node scripts/create-sitemap.js",
    "generate-robots": "node scripts/generate-robots.js",
    "analyze": "vite build --mode analyze",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@legendapp/state": "^2.1.15",
    "framer-motion": "^10.17.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sharp": "^0.33.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-static-copy": "^1.0.1",
    "vitest": "^1.0.4",
    "workerpool": "^8.0.0"
  }
}
